{
open YC.FST.AbstractLexing.Interpreter
open YC.FSA.FsaApproximation
}

include "../../GrammarTemplates/Common.yrd"

tokens {
    _ of FSA<char*Position<JetBrains.ReSharper.Psi.Tree.ILiteralExpression>>
}

options {
    translate = true
    module = "JSON.AbstractParser"
    infEpsPath = epsilons
    pos = "array<Position<JetBrains.ReSharper.Psi.Tree.ILiteralExpression>>" 
}												   

module Main

open Common

[<Start>]
value: STRING1 | NUMBER  | objects | array1  | 'true' | 'false' | 'null' 

array1: '[' list<value ','> ']' 

pair: STRING1 ':' value 
 
objects: '{' list<pair ','> '}'
