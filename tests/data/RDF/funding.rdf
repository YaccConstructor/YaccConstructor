  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Vendor">
    <rdfs:comment xml:lang="en">An agent who supplies goods or services in exchange for payment, in response to receipt of payment or of an official purchase order from a purchaser that is responded to by submission of an invoice for what is supplied.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Supplier"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">vendor</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/BudgetedAmount">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/BudgetInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">The amount of money originally available to spend in a particular budget category.</rdfs:comment>
    <rdfs:label xml:lang="en">budgeted amount</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/AvailableFunds">
    <rdfs:comment xml:lang="en">The amount available to spend in a budget category, equalling the income minus the expenditure to date plus future expenditure commitments. </rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/BudgetInformation"/>
    <rdfs:label xml:lang="en">available funds</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/RegistrationAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:label xml:lang="en">registration authority</rdfs:label>
    <rdfs:comment xml:lang="en">An agency with responsibility for registering and maintaining international standards, internet domain names, patents, controlled vocabularies and other similar entities required for reliable international cooperatative activities.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Repository">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A facility in which entities can be deposited for long-term preservation.</rdfs:comment>
    <rdfs:label xml:lang="en">repository</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Facility"/>
    <dc:description xml:lang="en">May also be described as an archive, or as a database.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ComputationalService">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Service"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A service provided by a computer system.</rdfs:comment>
    <rdfs:label xml:lang="en">computational service</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/usesService">
    <rdfs:label xml:lang="en">uses service</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Service"/>
    <rdfs:comment xml:lang="en">An object property linking an activity such as an expedition, a research project or an investigation, to a service employed in that activity.</rdfs:comment>
    <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasFamilialSuffix">
    <rdfs:label xml:lang="en">has familiar suffix</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasNameSuffix"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">The letters that may be displayed after a person's name to indicate familial status, e.g. Sn., Jn., III, 3rd.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/contributions-in-kind">
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:label xml:lang="en">contributions in kind</rdfs:label>
    <rdfs:comment xml:lang="en">A budget category to categorize the financial worth of contributions in kind (e.g. staff effort, access to facilities, sharing of know-how) given to a project by an agent, for example by the host institution or by a commerical partner in an academic project. </rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasQuotationDate">
    <rdfs:comment xml:lang="en">A data property linking a quotation to its date.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has quotation date</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/directly-allocated-costs">
    <rdfs:label xml:lang="en">directly allocated costs</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:comment xml:lang="en">A budget category to cover those costs normally borne by an institution, that are assigned to the budget of an externally funded project to cover the value of contributions to that project, for example to pay for the salary of a permanent member of staff seconded to work on the project, to compensate the employing organization for the loss of that person's efforts on other activities that he or she would otherwise be undertaking were it not for that secondment.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasLanguageCode">
    <rdfs:comment xml:lang="en">A data property specifying the two-letter or three-letter lower-case code used to identify a language (e.g. fr/fra for French, de/deu for Germany) (see http://en.wikipedia.org/wiki/List_of_ISO_639-1_codes).</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasCode"/>
    <rdfs:label xml:lang="en">has language code</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasProfessionalName">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdfs:label xml:lang="en">has alternate name</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">An alternative name that a person uses for professional purposes, for example the pen name of an author, or a married woman's maiden name used as her professional name. </rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasORCID">
    <rdfs:comment xml:lang="en">A data property relating an individual to his/her Open Researcher and Contributor ID (ORCID), an alphanumeric identifier for individual scholars described at http://about.orcid.org/.</rdfs:comment>
    <rdfs:label xml:lang="en">has ORCID</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalIdentifier"/>
    <dc:description xml:lang="en">owl:equivalentProperty &lt;http://purl.org/spar/scoro/hasORCID&gt; .</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ResearchInformationSystem">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/ComputationalService"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A database containing administrative information about the research undertaken in an institution, and the publications and other outputs from such research.</rdfs:comment>
    <rdfs:label xml:lang="en">research information system</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasHeadcount">
    <rdfs:label xml:lang="en">has headcount</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasOrganizationalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property specifying the number of people employed by an organization.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasModelNumber">
    <rdfs:comment xml:lang="en">A data property linking something, for example a piece of equipment, to its model number.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:label xml:lang="en">has model number</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Account">
    <rdfs:label xml:lang="en">account</rdfs:label>
    <rdfs:comment xml:lang="en">A mechanism for administering money, typically owned by an agent and dedicated to a particular purpose, for example the equipment account of a university department, to be used only for the purchase or maintenance of items of equipment. </rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/FundingProgramme">
    <rdfs:comment xml:lang="en">A programme of related funded activities or projects, typically established by a funding agency.  Often associated with publication of a call for applications (fabio:CallForApplications).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">funding programme</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Group">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/family_name">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N13ead28bcdaa47d3bdfab7ffc6ef6c35">
    <rdf:first rdf:resource="http://purl.org/cerif/frapo/Funding"/>
    <rdf:rest rdf:nodeID="N45afef4787ac43579c64d0d172db8968"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasExchangeRate">
    <rdfs:label xml:lang="en">has exchange rate</rdfs:label>
    <rdfs:comment xml:lang="en">A data property specifying the currency exchange rate between two specified currencies.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/DataRepository">
    <rdfs:label xml:lang="en">data repository</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Repository"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">May also be described as a data archive, or as a database.</dc:description>
    <rdfs:comment xml:lang="en">A facility in which digital data are deposited for long-term preservation and (optional) publication.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/NotForProfitOrganization">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">not-for-profit organization</rdfs:label>
    <rdfs:comment xml:lang="en">An organization that does not primarily exist to make profit, for example a charity or charitable foundation, a learned society, or a standards body such as the World Wide Web Consortium.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ResearchGroup">
    <rdfs:comment xml:lang="en">A group of people, usually within a university department or a research institute, and typically under a single group leader, dedicated to working together to undertake research on one or more topics. May also be called a research unit.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">research group</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Group"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasDomainName">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has domain name</rdfs:label>
    <rdfs:comment xml:lang="en">A data property linking an entity to an internet domain name that has been registered for that entity, for example http://www.miidi.org for MIIDI, the Minimal Information standard for reporting an Infectious Disease Investigation.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/surname">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/sellsTo">
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/purchasesFrom"/>
    <rdfs:comment xml:lang="en">An object property relating a supplier to a purchaser to whom an entity is sold.</rdfs:comment>
    <rdfs:label xml:lang="en">sells to</rdfs:label>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <dc:description xml:lang="en">Domain and range constraints on this object property have been relaxed to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/description">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/BudgetInformation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">Information about an aspect of a budget.</rdfs:comment>
    <rdfs:label xml:lang="en">budget information</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ScholarshipApplication">
    <rdfs:label xml:lang="en">scholarship application</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Application"/>
    <rdfs:comment xml:lang="en">The activity of applying for an educational scholarship.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasMilestone">
    <rdfs:label xml:lang="en">has milestone</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property that permits one to define a significant event (a 'milestone') in the progress of a project.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/cerro/results_in_beginning">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/MaterialOutput">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A material or physical output created or produced as a result of a project or an endeavour such as a research investigation. Examples include a building constructed as a result of a building project, and research outputs such as a cell line, a mouse mutant or a new form of microscope.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Output"/>
    <rdfs:label xml:lang="en">material output</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/purchases">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">purchases</rdfs:label>
    <dc:description xml:lang="en">Range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/isPurchasedBy"/>
    <rdfs:comment xml:lang="en">An object property relating a purchaser to a purchased entity.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Laboratory">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Facility"/>
    <rdfs:label xml:lang="en">laboratory</rdfs:label>
    <rdfs:comment xml:lang="en">A facility in which research can be conducted.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCurrencyCode">
    <rdfs:comment xml:lang="en">A data property specifying the three-letter currency code used to identify a currency, as defined by ISO 4217 (e.g. EUR or GBP) (see http://en.wikipedia.org/wiki/ISO_4217).  

Not to be confused with the symbol used with numbers to denote an amount of money in a particular currency (e.g.500; 3.40)</rdfs:comment>
    <rdfs:label xml:lang="en">has currency code</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasCode"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/business-angel">
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:label xml:lang="en">business angel</rdfs:label>
    <rdfs:comment xml:lang="en">A person willing to invest in start-up companies.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasDeliveryDate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:label xml:lang="en">has delivery date</rdfs:label>
    <rdfs:comment xml:lang="en">The date on which something is delivered, or is due to be delivered.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/awards">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment xml:lang="en">An object property linking an agent to something the agent awards, for example linking a funding agency to a grant, a university to a degree, or an organization to a prize.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">awards</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nfac159c4902440b9b9589f50cb24f95a">
    <rdf:rest rdf:nodeID="Na9ad9ba4550c4a378143c9e7f8d45b89"/>
    <rdf:first rdf:resource="http://purl.org/cerif/frapo/Funding"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/academic">
    <rdfs:label xml:lang="en">academic</rdfs:label>
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">A member of the academic staff of an academic institution such as a university.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasSerialNumber">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:label xml:lang="en">has serial number</rdfs:label>
    <rdfs:comment xml:lang="en">A data property linking something, for example a piece of equipment, to its serial number.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Endeavour">
    <rdfs:label xml:lang="en">endeavour</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en">In FRAPO, an endeavour is an activity such as a research investigation that is enabled by a project.  An endeavour relate to the academic and practical aspects of undertaking that endeavour - thus for a research investigation the research itself, including the hypotheses being investigated, the experiments being undertaken, the data being gathered and analysed, the interpretations being drawn from the data, and the results obtained.  In contrast, the project relates to the administrative aspects of undertaking an endeavour, including the funding obtained, the people employed, and the management of the project in terms of deadlines and deliverables.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Purchaser">
    <rdfs:label xml:lang="en">purchaser</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment xml:lang="en">An agent who purchases something from a supplier.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasQualification">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdfs:comment xml:lang="en">A data property permitting specification of the qualification held by a person.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has qualification</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/BudgetCategory">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">budget category</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdfs:comment xml:lang="en">A category of expenditure within a budget, determining what the allocated money can be spent on, for example salaries, equipment, travel.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasFaxNumber">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasAgentInformation"/>
    <rdfs:label xml:lang="en">has fax number</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property identifying a Fax number for an agent.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/benefactor">
    <rdfs:comment xml:lang="en">A person who gives a gift.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:label xml:lang="en">benefactor</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Status">
    <rdfs:label xml:lang="en">status</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en">The status of something such as an application, an investigation or a project.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2006/timezone#TimeZone">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has time zone</rdfs:label>
    <rdfs:comment xml:lang="en">A data property that permits the time zone relating to an entity to be specified.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasDegreeSuffix">
    <rdfs:label xml:lang="en">has degrees</rdfs:label>
    <rdfs:comment xml:lang="en">The letters that should be displayed after a person's name to indicate an academic degree, e.g. BA, BVSc, PhD, DSc.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasNameSuffix"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ConsultancyAgreement">
    <rdfs:comment xml:lang="en">A contract between an employer and a consultant, detailing the terms of the consultancy.</rdfs:comment>
    <rdfs:label xml:lang="en">consultancy agreement</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Contract"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCompletionDate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">A data property linking something such as a building or a contract to its date of completion.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has completion date</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/FinancialEntity">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en">financial entity</rdfs:label>
    <rdfs:comment xml:lang="en">An entity primarily to do with money.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Studentship">
    <rdfs:comment xml:lang="en">A financial grant to a student to enable him/her to undertake advanced academic study.</rdfs:comment>
    <rdfs:label xml:lang="en">studentship</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Stipend"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/entrepreneur">
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:label xml:lang="en">A person who invests in novel enterprises.</rdfs:label>
    <rdfs:label xml:lang="en">entrepreneur</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/givenname">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/givenNameInitial">
    <rdfs:label xml:lang="en">given name initial</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property permitting specification of the initial(s) of a person's given name(s).</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/initial"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/PostalAddress">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en">postal address</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A postal address.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ConferenceFee">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Fee"/>
    <rdfs:label xml:lang="en">conference fee</rdfs:label>
    <rdfs:comment xml:lang="en">A fee paid to attend a conference.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Company">
    <rdfs:label xml:lang="en">company</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A commercial company or business run for profit.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Organization">
    <dc:description xml:lang="en">owl:equivalentClass cerf:OrganizationalUnit .</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/identifier">
    <rdfs:label xml:lang="en">identifier</rdfs:label>
    <rdfs:comment xml:lang="en">A unique and unambiguous reference to the resource within a given context.

Recommended best practice is to identify the resource by means of a string conforming to a formal identification system.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isDonatedBy">
    <rdfs:label xml:lang="en">is donated by</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking a gift to the benefactor who provided it.</rdfs:comment>
    <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/donates"/>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/AccountStatement">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdfs:label xml:lang="en">account statement</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A statement of income and expenditure for an account, typically presented as dated income receipts and outgoings against categories in a budget for that account.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasSupplierNumber">
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Supplier"/>
    <rdfs:label xml:lang="en">has supplier number</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:comment xml:lang="en">A data property linking a supplier to an identifying number for that supplier, typically used by the accounting system of the purchasing institution when placing and tracking orders.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasAwardDate">
    <rdfs:comment xml:lang="en">The date on which notification is given or received that something, for example a scholarship or a grant, has been awarded.  [See also frapo:hasDecisionDate.]</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has award date</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/accepted">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/Status"/>
    <rdfs:comment xml:lang="en">The status of something such as an application as having been accepted.</rdfs:comment>
    <rdfs:label xml:lang="en">accepted</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/consultancy-costs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">consultancy costs</rdfs:label>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:comment xml:lang="en">A budget category to cover the costs of consultancy fees.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nfae040d896c94f1581c6b79f9d1416a9">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:unionOf rdf:nodeID="N13ead28bcdaa47d3bdfab7ffc6ef6c35"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasGrantNumber">
    <rdfs:label xml:lang="en">has grant number</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Grant"/>
    <rdfs:comment xml:lang="en">A data property specifying the grant number of a grant provided by a funding agency.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/GovernmentOrganization">
    <rdfs:comment xml:lang="en">An organization that is part of, or funded by, central or local goverment.</rdfs:comment>
    <rdfs:label xml:lang="en">government organization</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPhoneNumber">
    <rdfs:label xml:lang="en">has phone number</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasAgentInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property identifying a telephone number for an agent.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Budget">
    <rdfs:label xml:lang="en">budget</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdfs:comment xml:lang="en">A structured statement of how funds are to be used for expenditure in defined budget categories.  A budget is created, for example, when applying for funding for a project, and is subsequently used to manage the expenditure from the awarded grant for that project.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Project">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">In FRAPO, a project is an administrative entity that enables an endeavour such as a research investigation.  A project relates to the administrative aspects of undertaking an endeavour, including the funding obtained, the people employed, and the management of the project in terms of deadlines and deliverables.  In contrast, an endeavour such as a research investigation relate to the academic and practical aspects of undertaking that endeavour - thus for a research investigation: the research itself, including the hypotheses being investigated, the experiments being undertaken, the data being gathered and analysed, the interpretations being drawn from the data, and the results obtained.</rdfs:comment>
    <dc:description xml:lang="en">Super-class of cerif:Project.</dc:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en">project</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasMonetaryValue">
    <rdfs:comment xml:lang="en">A data property enabling one to specify the monetary value of a financial entity such as a grant, a gift or a budget category.  Use in conjuction with frapo:hasCurrency.</rdfs:comment>
    <rdfs:label xml:lang="en">has value</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/submits">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">submits</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking an agent to an entity that is submitted to some third party, for example a grant application submitted to a funding agency, a manuscript submitted to a publisher, a quotation submitted by a vendor, or a dataset submitted by a researcher to a data repository.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ServiceContractFee">
    <rdfs:comment xml:lang="en">A fee paid periodically, typically annually, to obtain access when required to a service such as an equipment maintenance contract."</rdfs:comment>
    <rdfs:label xml:lang="en">service contract fee</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Fee"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasDeliverable">
    <rdfs:label xml:lang="en">has deliverable</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property that permits one to define a deliverable of a project.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/software-costs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">software costs</rdfs:label>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:comment xml:lang="en">A budget category to cover the costs of software licenses required by a project.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Deliverable">
    <rdfs:label xml:lang="en">deliverable</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Output"/>
    <rdfs:comment xml:lang="en">A class defining a 'deliverable' of a project, i.e. something that the project set out to deliver, or succeeds in delivering.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/FinancialControlSystem">
    <rdfs:label xml:lang="en">financial control system</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/ComputationalService"/>
    <rdfs:comment xml:lang="en">A software system controlling the financial activities of an organization, for example determining full economic cost budgets for grant applications, and managing the funding of funded projects.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPersonalInformation">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasAgentInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:label xml:lang="en">has contact information</rdfs:label>
    <rdfs:comment xml:lang="en">A data property linking a person to information about that person.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasDispatchDate">
    <rdfs:comment xml:lang="en">The date on which something, for example an item of equipment, is dispatched by the supplier.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:label xml:lang="en">has dispatch date</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCityOrTown">
    <rdfs:comment xml:lang="en">A data property specifying a city or town, for example as the location of an event.</rdfs:comment>
    <rdfs:label xml:lang="en">has city or town</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasHonorificSuffix">
    <rdfs:comment xml:lang="en">The letters that should be displayed after a person's name to indicate qualification, membership or fellowship in a professional society, honours or elite status, e.g. MRCVS, FRCS, FRS, OBE, Kt.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">post-nominals</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasNameSuffix"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ExpenditureToDate">
    <rdfs:comment xml:lang="en">Total money expended to date from an an account, or from a particular budget category.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">expenditure to date</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Expenditure"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCountry">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property specifying a country, for example as the location of an event.</rdfs:comment>
    <rdfs:label xml:lang="en">has country</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ServiceContract">
    <rdfs:label xml:lang="en">service contract</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Contract"/>
    <rdfs:comment xml:lang="en">A contract for the provision of a service such as a cleaning service, or for the regular servicing, maintenance and repair of some item of equipment or infrastructure.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/travel-and-subsistence-costs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">A budget category to cover the costs of travel and subsistence incurred on behalf of the project.</rdfs:comment>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:label xml:lang="en">travel and subsistence costs</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Stipend">
    <rdfs:comment xml:lang="en">A sum of money allotted on a regular basis, usually for some specific purpose.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Funding"/>
    <rdfs:label xml:lang="en">stipend</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/SpinOffCompany">
    <rdfs:label xml:lang="en">spin-off company</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Company"/>
    <rdfs:comment xml:lang="en">A company that has been established by a university or research institute to mature and market the results of research conducted at that research institution.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/nick">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Grant">
    <rdfs:label xml:lang="en">grant</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Funding"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A financial award by a funding agency for a particular project or purpose, usually awarded on the basis of a competitive application.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/non-staff-costs">
    <rdfs:label xml:lang="en">non-staff costs</rdfs:label>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:comment xml:lang="en">A budget category to cover all the costs associated with a project that do not involve the employment of staff by the institution, thus, for example, including consumables purchase, travel costs and consultancy fees, but excluding the salary, estates and indirect costs associated with the allocation of the PI's time to the project and the direct employment of a project manager.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasFundingAgency">
    <rdfs:comment xml:lang="en">An object property linking something to a funding agency that provides funding for it.</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/FundingAgency"/>
    <rdfs:label xml:lang="en">has funding agency</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/isSupportedBy"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/HostInstitution">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">host institution</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:comment xml:lang="en">An institution that acts as a host, for example of a project or a person.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/University">
    <rdfs:comment xml:lang="en">A university at which higher education and research is conducted.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:label xml:lang="en">university</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/title">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Legacy">
    <rdfs:comment xml:lang="en">A gift of money or property specified in the will of a person, that is made after that person's death.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Gift"/>
    <rdfs:label xml:lang="en">legacy</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Fellowship">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">fellowship</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Stipend"/>
    <rdfs:comment xml:lang="en">The stipend paid to a fellow in a college or university.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Business_angel">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/FundingApplication">
    <rdfs:label xml:lang="en">funding application</rdfs:label>
    <rdfs:comment xml:lang="en">The activity of applying for funding.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Application"/>
    <dc:description xml:lang="en">For the documents supporting a funding application, use fabio:GrantApplicationDocument and fabio:CaseForSupportDocument.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ComputationalAgent">
    <rdfs:comment xml:lang="en">A computer system or software program whose action has an effect on external events, for example a laboratory robot that undertakes your experiments, or the financial control system that controls an institution's payrole, or that, following programmed business rules, prevents the principal investigator from placing an order for research equipment because there is insufficient money left in the project budget.</rdfs:comment>
    <rdfs:label xml:lang="en">computational agent</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ProjectBudget">
    <rdfs:comment xml:lang="en">A budget for a research project.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">project budget</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Budget"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCurrency">
    <rdfs:comment xml:lang="en">A data property defining the currency used to specify a financial entity.  Use in conjection with frapo:hasMonetaryValue.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has currency</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Person">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Tender">
    <rdfs:label xml:lang="en">tender</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">Sealed bid or offer document submitted in response to a request for tenders, containing detailed information on costs, requirements and terms associated with a potential contract.  (Adapted from http://www.businessdictionary.com/definition/tender.html).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/feeOrFree">
    <rdfs:comment xml:lang="en">A data property defining whether participation in an event is free, or requires payment of a fee.</rdfs:comment>
    <rdfs:label xml:lang="en">fee or free</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Output">
    <rdfs:comment xml:lang="en">Something that is created or produced as a result of a project or an endeavour. Examples include a building resulting from a building project, educated students graduating from an educational programme, and a journal article published as a result of a research project.  Research outputs can also include reports, datasets, experimental protocols, workflows and mathematical models, and material outputs such as a cell line, a mouse mutant or a new form of microscope.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <dc:description xml:lang="en">Ontology terms and definitions for most publishable project outputs are given as 'Expression' classes in FaBiO, the FRBR-aligned Bibliographic Ontology, e.g. fabio:JournalArticle, fabio:DataSet.  For 'deliverables', use frapo:Deliverable.  For material outputs, use frapo:MaterialOutput.</dc:description>
    <rdfs:label xml:lang="en">output</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Equipment"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Document">
    <dc:description xml:lang="en">Ontology terms and definitions for many documents relevant to FRAPO are given as 'Expression' classes in FaBiO, the FRBR-aligned Bibliographic Ontology, e.g. fabio:CallForApplications, fabio:DataManagementPlan, fabio:GrantApplicationDocument, fabio:ProjectPlan.  Others are given as sub-classes of this class.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ResearchInstitute">
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">research institute</rdfs:label>
    <rdfs:comment xml:lang="en">An organization primarily dedicated to undertaking research, that might be part of a university or of a commercial company, or might be independently funded by a research funding agency.  </rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/appliesFor">
    <rdfs:label xml:lang="en">applies for</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking an agent to something for which that agent applies, for example a job, membership of an organization, or a grant from a funding agency.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/consumables-costs">
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:label xml:lang="en">consumables costs</rdfs:label>
    <rdfs:comment xml:lang="en">A budget category to cover the costs of purchasing consumables.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N45afef4787ac43579c64d0d172db8968">
    <rdf:first rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#seeAlso">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasAcronym">
    <rdfs:comment xml:lang="en">A data property linking an entity to an acronym used to identify it, based on its full name or title.  For example, this ontology has the full name 'Funding, Research Administration and Projects Ontology' and the acronym 'FRAPO'.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has acronym</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/service-costs">
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:label xml:lang="en">service costs</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">A budget category to cover the costs of services used by a project, for example a DNA sequencing service, or an equipment maintenance service.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Faculty">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Division"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">faculty</rdfs:label>
    <rdfs:comment xml:lang="en">A grouping of similar departments within a university context, also known as a school or a division, for example the Humanities Division, the Mathematics Faculty, or the School of Medicine.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Ne9d470a0aac8409eba134065b075a142">
    <rdf:first rdf:resource="http://purl.org/cerif/frapo/Budget"/>
    <rdf:rest rdf:nodeID="Ndd75ba70ca9c43eb9ae4a532bb471cf7"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Equipment">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/InfrastructureEntity"/>
    <rdfs:label xml:lang="en">equipment</rdfs:label>
    <rdfs:comment xml:lang="en">An item of equipment or apparatus, or a laboratory instrument, for example a refrigerated centrifuge.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/creator">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasIPROwner">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/isOwnedBy"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An object property linking an entity such as a project, an endeavour or an output to an agent who owns the IPR arising from or related to that entity.</rdfs:comment>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">has IPR owner</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Division">
    <rdfs:label xml:lang="en">division</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:comment xml:lang="en">A grouping of similar departments within a larger organization.  In the university context, also known as a school or a faculty, for example the Humanities Division, the Mathematics Faculty, or the School of Medicine.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Payment">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A financial payment made, for example in exchange for something purchased such as property, goods or services, or to refund expenses incurred.</rdfs:comment>
    <rdfs:label xml:lang="en">payment</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Ne8456f273fa641888f72a120402b6e53">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:unionOf rdf:nodeID="Nfac159c4902440b9b9589f50cb24f95a"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N88afa62a21a94fd09b199cae6ecd610f">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:unionOf rdf:nodeID="Ne9d470a0aac8409eba134065b075a142"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isSubmittedBy">
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment xml:lang="en">An object property linking an entity that is submitted (to some third party) to the agent who submits it.</rdfs:comment>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/submits"/>
    <rdfs:label xml:lang="en">is submitted by</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/date">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#versionInfo">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#date">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/PurchaseOrder">
    <rdfs:label xml:lang="en">purchase order</rdfs:label>
    <rdfs:comment xml:lang="en">An official order from a purchasing institution to a supplier for the purchase of goods or services, often issued in response to the receipt of a quotation from the supplier.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/birthday">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/owl:sameAs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Na9ad9ba4550c4a378143c9e7f8d45b89">
    <rdf:first rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/rights">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
</rdf:RDF>
