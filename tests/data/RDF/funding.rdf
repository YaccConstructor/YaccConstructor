<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
   xmlns:dc="http://purl.org/dc/elements/1.1/"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasBatchNumber">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:comment xml:lang="en">A data property linking something, for example a chemical or drug, to its manufacturing batch number.</rdfs:comment>
    <rdfs:label xml:lang="en">has batch number</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Service">
    <rdfs:label xml:lang="en">service</rdfs:label>
    <rdfs:comment xml:lang="en">A service of help to others, often requiring funding, employees, equipment and/or facilities for its provision, for example a library service, a transportation service, a DNA sequencing service.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/InfrastructureEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasStartDate">
    <rdfs:label xml:lang="en">has start date</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">A data property linking something such as a project to its start date.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/funds">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/supports"/>
    <rdfs:comment xml:lang="en">An object property that links a grant to something that it funds (i.e. that it finances or pays for), or that links an agent providing funding to something that it funds.  This flexibility is given because of alternative usages of the English word 'funds'.  [See also frapo:awards and frapo:supports.]</rdfs:comment>
    <rdfs:label xml:lang="en">funds</rdfs:label>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/isFundedBy"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Funding">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">funding</rdfs:label>
    <rdfs:comment xml:lang="en">An amount of money available to finance some project or activity. </rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasLocation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has location</rdfs:label>
    <rdfs:comment xml:lang="en">A data property link an entity to a relevant location, which might be geographical (e.g. New York City) or local (e.g. Freezer B, second shelf).</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasReferenceNumber">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has reference number</rdfs:label>
    <rdfs:comment xml:lang="en">A data property linking something to a reference number for that item.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCatalogNumber">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:comment xml:lang="en">A data property linking something to its catalog number.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has catalog number</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#label">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasAccountNumber">
    <rdfs:label xml:lang="en">has account number</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property linking a financial account to an identifying number for that account.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Account"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isSuppliedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">is supplied by</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Supplier"/>
    <rdfs:comment xml:lang="en">An object property relating an entity that is supplied, or an agent to whom an entity is supplied, to the supplier.</rdfs:comment>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/supplies"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Owner">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment xml:lang="en">The agent who owns an entity.</rdfs:comment>
    <rdfs:label xml:lang="en">owner</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasBirthDate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/birthday"/>
    <rdfs:label xml:lang="en">has birth date</rdfs:label>
    <rdfs:comment xml:lang="en">A data property permitting specification of the birth date of a person.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Supplier">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">An agent who supplies something, such as goods or services.</rdfs:comment>
    <rdfs:label xml:lang="en">supplier</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ConsortiumAgreement">
    <rdfs:label xml:lang="en">consortium agreement</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A formal agreement between members of a consortium, for example between research groups at different universities engaged in a multi-partner research project.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Contract"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPurchaseOrderNumber">
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/PurchaseOrder"/>
    <rdfs:label xml:lang="en">has purchase order number</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property identifying an official purchase order issued by a purchasing agent to a supplier of goods or services, and used by the supplier to identify the order when delivering goods or submitting an invoice.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasApplicationOutcome">
    <rdfs:comment xml:lang="en">A data property that permits the outcome of an application to be described, for example that a job has been offered or that a grant has been awarded.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:label xml:lang="en">has application outcome</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/SME">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Company"/>
    <rdfs:label xml:lang="en">small to medium enterprise</rdfs:label>
    <rdfs:comment xml:lang="en">A small to medium enterprise, having up to 250 employees.</rdfs:comment>
    <rdfs:label xml:lang="en">sme</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/providesFractionOfTotalFunding">
    <rdfs:label xml:lang="en">percentage of total funding</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking funding to a budget, specifying what fraction of the total budget is provided by that source of funding.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Funding"/>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Budget"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Manufacturer">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">manufacturer</rdfs:label>
    <rdfs:comment xml:lang="en">An agent who manufactures goods or equipment.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/contributor">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/familyNameInitial">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/initial"/>
    <rdfs:label xml:lang="en">family name initial</rdfs:label>
    <rdfs:comment xml:lang="en">A data property permitting specification of the initial(s) of a person's family name.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/created">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment xml:lang="en">A data property linking something to its date of creation.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has creation date</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/College">
    <rdfs:label xml:lang="en">college</rdfs:label>
    <rdfs:comment xml:lang="en">An academic organization that is either a small degree-giving university, or (in the OxBridge sense) an independent corporation of scholars within a larger university, providing undergraduate tuition and a social environment for its students and fellows.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPurchaseDate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:label xml:lang="en">has purchase date</rdfs:label>
    <rdfs:comment xml:lang="en">A data property linking something to its purchase date.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/funded">
    <rdfs:comment xml:lang="en">The status of something such as an investigation or a project as being funded.</rdfs:comment>
    <rdfs:label xml:lang="en">funded</rdfs:label>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/Status"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasInvoiceDate">
    <rdfs:comment xml:lang="en">A data property linking an invoice to its date.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:label xml:lang="en">has invoice date</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Invoice"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/directly-incurred-costs">
    <dc:description xml:lang="en">Also known as the 'direct costs' of the project.</dc:description>
    <rdfs:comment xml:lang="en">A budget category to cover all costs directly incurred because of a legitimate expenditure on a project, for example the purchase of reagents or the employment of a post-doctoral researcher.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">directly incurred costs</rdfs:label>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/manufactures">
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Manufacturer"/>
    <rdfs:label xml:lang="en">manufactures</rdfs:label>
    <rdfs:comment xml:lang="en">An object property relating a manufacturer to a manufactured entity.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/DocumentRepository">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Repository"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A facility in which digital documents are deposited for long-term preservation and (optional) publication.</rdfs:comment>
    <dc:description xml:lang="en">May also be described as a document archive, or a digital library.</dc:description>
    <rdfs:label xml:lang="en">document repository</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/">
    <dc:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Silvio Peroni</dc:contributor>
    <dc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FRAPO, the Funding, Research Administration and Projects Ontology</dc:title>
    <dc:rights xml:lang="en">This work is distributed under a Creative Commons Attribution License (http://creativecommons.org/licenses/by/3.0/).</dc:rights>
    <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2014-01-31</dc:date>
    <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FRAPO is an ontology for use to describe research project administrative information and to work with CRIS (Current Research Information Systems). It can be used for the characterization of grant applications, funding bodies, projects, project partners, etc.  It can also be used to describe other types of projects, for example building projects and educational projects.  

FRAPO imports FOAF and is designed to work with SCoRO, the Scholarly Contributions and Roles Ontology (http://purl.org/spar/scoro/), which is used to characterize the contributions and time-limited roles that may exist between agents such as people and organizations and entities such as projects, research investigations or research outputs.  

FaBiO, the FRBR-alightned bibliographic ontology (http://purl.org/spar/fabio/), provides terms for documents of relevance, such as grant applications, project plans, project reports, datasets and journal articles, which are therefore not included within FRAPO.

FRAPO is not intended to describe the details of research investigations enabled by funded research projects, nor their component studies and assays, which are the domain of a complementary ontology, ISA-RDF (under development - add URL).</dc:description>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.1</owl:versionInfo>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">David Shotton</dc:creator>
    <owl:imports rdf:resource="http://purl.org/swan/2.0/foaf-essential/"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FRAPO, the Funding, Research Administration and Projects Ontology, is a CERIF-compliant ontology  written in OWL 2 DL for describing research project administrative information.  This ontology is available at http://purl.org/cerif/frapo/, and uses the namespace prefix 'frapo'.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/enables">
    <dc:description xml:lang="en">Domain and range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:label xml:lang="en">enables</rdfs:label>
    <rdfs:comment xml:lang="en">An object property relating an entity to another entity that it enables, e.g. linking a project to an endeavour that the project enables.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasRegistrationDeadline">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:label xml:lang="en">has registration deadline</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">The date by which registration must be made, for example renewal of an internet domain name.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/usesEquipment">
    <rdfs:label xml:lang="en">uses equipment</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An object property linking an activity such as an expedition, a research project or an investigation, to an item of equipment employed in that activity.</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Equipment"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Library">
    <rdfs:comment xml:lang="en">A facility in which printed documents (for example books and journals) are deposited for long-term preservation and access.</rdfs:comment>
    <rdfs:label xml:lang="en">library</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Repository"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isEvaluatedBy">
    <rdfs:comment xml:lang="en">An object property linking an entity to an agent that evaluates it.</rdfs:comment>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/evaluates"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">is evaluated by</rdfs:label>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/RegistrationAgency">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">An organization that acts to register things.  For example, CrossRef is a registration agency for DOIs relating to documents, while DataCite is a registration agency for DOIs relating to datasets.</rdfs:comment>
    <rdfs:label xml:lang="en">registration agency</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Purchase">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">The payment for obtaining something, for example property, goods or services.</rdfs:comment>
    <rdfs:label xml:lang="en">purchase</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/employs">
    <dc:description xml:lang="en">This property should not be used for the other meaning of 'employ', meaning to use something, e.g. 'He employed great skill . . .'/</dc:description>
    <rdfs:comment xml:lang="en">An object property relating an employing agent to an employed agent, for example a person or an organization, who is typically employed to undertaking specified work in exchange for payment.</rdfs:comment>
    <rdfs:label xml:lang="en">employs</rdfs:label>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isEmployedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/employs"/>
    <rdfs:label xml:lang="en">is employed by</rdfs:label>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment xml:lang="en">An object property relating an employed agent, typically a person or an organization, to an employing agent.</rdfs:comment>
    <dc:description xml:lang="en">This property should not be used for the other meaning of 'employ', meaning to use something, e.g. 'Great skill is employed by master craftsmen . . .'/</dc:description>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Facility">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/InfrastructureEntity"/>
    <rdfs:label xml:lang="en">facility</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A place or installation built or designed to serve a specific function or provide a specific service, for example an animal breeding centre, a DNA sequencing facility or a confocal imaging laboratory.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasFunderIdentifier">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has funder identifer</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/FundingAgency"/>
    <rdfs:comment xml:lang="en">A data property linking a funding agency to an identifier for that funding agency.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPersonalIdentifier">
    <rdfs:comment xml:lang="en">A unique and unambiguous identifier for an individual person within a given context.</rdfs:comment>
    <dc:description>owl:equivalentProperty &lt;http://purl.org/spar/datacite/hasPersonalIdentifier&gt; .</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <dc:description xml:lang="en">owl:equivalentProperty &lt;http://purl.org/spar/scoro/hasPersonalIdentifier&gt; .</dc:description>
    <rdfs:label xml:lang="en">has personal identifier</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/rejected">
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/Status"/>
    <rdfs:label xml:lang="en">rejected</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">The status of something such as an application as having been rejected.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isSupportedBy">
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/supports"/>
    <dc:description xml:lang="en">This property is not intended for use to describe mechanical or physical support, as in 'The statue is supported by the plinth'. </dc:description>
    <rdfs:label xml:lang="en">is supported by</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An object property linking something to an agent that provides funding or other support for it.</rdfs:comment>
    <rdfs:range rdf:nodeID="Ne8456f273fa641888f72a120402b6e53"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasExpiryDate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">A data property linking something, for example a drug, to its expiry date, beyond which the manufacture recommends that it should not be used.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:label xml:lang="en">has expiry date</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPostCode">
    <rdfs:label xml:lang="en">has post code</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasCode"/>
    <rdfs:comment xml:lang="en">A data property defining the postal code of an address.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Quotation">
    <rdfs:label xml:lang="en">quotation</rdfs:label>
    <rdfs:comment xml:lang="en">A formal statement of promise (usually provided in response to a request for quotation) by potential supplier to supply goods or services at specified prices and within a specified period. Acceptance of the quotation by the buyer constitutes an agreement binding on both parties.  (Shortened from http://www.businessdictionary.com/definition/quotation.html).
</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Subscription">
    <rdfs:label xml:lang="en">subscription</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Fee"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">Payment, commonly made annually, to obtain access to something on a periodic or continuing basis, such as a journal.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/gender">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Investigation">
    <dc:description xml:lang="en">Equivalent class in ISA-RDF: 'Investigation'.</dc:description>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Endeavour"/>
    <rdfs:comment xml:lang="en">An investigation is an endeavour that involves research, enabled by a project.  

The investigation relate to the scholarly and practical aspects of conducting the research itself, including the hypotheses being investigated, the experiments being undertaken, the data being gathered and analysed, the interpretations being drawn from the data, and the results obtained.  

In contrast, the project that enables it encompasses the administrative aspects of undertaking the endeavour, including the funding obtained, the people employed, and the management of the project in terms of deadlines and deliverables.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">investigation</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isOutputOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">is output of</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking something that is created or produced to the activity that created or produced it.</rdfs:comment>
    <dc:description xml:lang="en">Domain and range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/hasOutput"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Fee">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">fee</rdfs:label>
    <rdfs:comment xml:lang="en">An amount of money paid for something that is not purchased.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Ndd75ba70ca9c43eb9ae4a532bb471cf7">
    <rdf:first rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPostalAddressLine">
    <rdfs:comment xml:lang="en">A data property permitting specification of a single line of a postal address.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasAgentInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/PostalAddress"/>
    <rdfs:label xml:lang="en">has postal address line</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasKeyword">
    <rdfs:label xml:lang="en">has keyword</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property permitting specification of a keyword relating to an entity.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/evaluates">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">evaluates</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking an agent to an entity which that agent evaluates, for example a funding agency that is evaluating a grant application to determine whether the proposed project is worthy of funding, or a publisher evaluating a manuscript to determine whether it is suitable for publication.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasTemporalDuration">
    <rdfs:comment xml:lang="en">A data property enabling one to define the temporal duration of some entity such as an event or a project.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:label xml:lang="en">has temporal duration</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isOwnedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An object property linking an entity to the agent that owns it.</rdfs:comment>
    <rdfs:label xml:lang="en">is owned by</rdfs:label>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/owns"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/AnnualTurnover">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">The anual financial turnover of an organization (in a specified currency).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdfs:label xml:lang="en">annual turnover</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCustomerNumber">
    <rdfs:comment xml:lang="en">A data property linking an agent to an identifying number for that agent, typically used by the accounting system of the supplier when issuing quotations, fulfilling orders and submitting invoices.</rdfs:comment>
    <rdfs:label xml:lang="en">has customer number</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Purchaser"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isAwardedBy">
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/awards"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:label xml:lang="en">is awarded by</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking something awarded to the agent making the award, for example linking a grant to a funding agency, a degree to a university, or a prize to the awarding organization.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/usesFacility">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">uses facility</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Facility"/>
    <rdfs:comment xml:lang="en">An object property linking an activity such as an expedition, a research project or an investigation, to a facility employed in that activity.</rdfs:comment>
    <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPostalAddress">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasAgentInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has postal address</rdfs:label>
    <rdfs:comment xml:lang="en">A data property identifying a postal address for an agent.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/InfrastructureEntity">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <dc:description xml:lang="en">owl:equivalentClass cerif:InfrastructureEntity .</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">infrastructure entity</rdfs:label>
    <rdfs:comment xml:lang="en">An entity that facilitates work.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ArticleProcessingCharge">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">article processing charge</rdfs:label>
    <rdfs:comment xml:lang="en">A fee levied by a publisher on an author or an institution to cover the cost of publishing an Open Access journal article.  Also knows as Article Processing Fee, Article Publication Charge, Author Processing Charge or Author Publishing Charge.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Fee"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasJobTitle">
    <rdfs:comment xml:lang="en">A data property permitting specification of the job title held by a person, such as might appear next to the name in a directory or on a conference name badge.  For example, the person's specific Job Title might be 'Linacre Professor of Zoology' or 'Regius Professor of Moral Philosophy'. 

This job title properly refers to the **role** held by that person (e.g. 'Head of Department'), which can be separately defined using SCoRO, the Scholarly Contributions and Roles Ontology (http://purl.org/spar/scoro/). [see scoro:withJobTitle].

</rdfs:comment>
    <rdfs:label xml:lang="en">has job title</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCountryCode">
    <rdfs:label xml:lang="en">has country code</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property specifying the two-letter or three-letter upper-case country code used to identify a country (e.g. FR/FRA for France, DE/DEU for Germany) (see http://countrycode.org/).</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasCode"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/facilities-costs">
    <rdfs:comment xml:lang="en">A budget category to cover the costs of maintaining facilities used by a project, for example an electron microscopy facility.</rdfs:comment>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:label xml:lang="en">facilities costs</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/name">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isManufacturedBy">
    <rdfs:label xml:lang="en">is manufactured by</rdfs:label>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/manufactures"/>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Manufacturer"/>
    <rdfs:comment xml:lang="en">An object property relating a manufactured entity to its manufacturer.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isPurchasedBy">
    <rdfs:comment xml:lang="en">An object property relating a purchased entity to its purchaser.</rdfs:comment>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:label xml:lang="en">was purchased by</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/publishing-costs">
    <rdfs:label xml:lang="en">publishing costs</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">A budget category to cover the cost of publishing outpots from a project, for example author fees for journal articles in an open access journal.</rdfs:comment>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/other-costs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:label xml:lang="en">other costs</rdfs:label>
    <rdfs:comment xml:lang="en">A budget category to cover costs not covered in other budget categories.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasExpertise">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasAgentInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has expertise</rdfs:label>
    <rdfs:comment xml:lang="en">A data property identifying the area(s) of expertise and skills posessed by an individual or an organization.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Commitments">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/BudgetInformation"/>
    <rdfs:label xml:lang="en">commitments</rdfs:label>
    <rdfs:comment xml:lang="en">Money set aside to honour expenditures already made but for which no invoices have yet been received, or expenditures planned for the future, for example for salaries.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ownsIRPOf">
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/hasIPROwner"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">owns IPR of</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking an agent who owns IPR arising from or relating to an entity to that entity.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/owns"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/purchasesFrom">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An object property relating a purchaser to a supplier from whom the purchase is made.</rdfs:comment>
    <rdfs:label xml:lang="en">purchases from</rdfs:label>
    <dc:description xml:lang="en">Domain and range constraints on this object property have been relaxed to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/full-economic-costs">
    <dc:description xml:lang="en">Frequently abbreviated 'FEC'.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">full economic costs</rdfs:label>
    <rdfs:comment xml:lang="en">A budget category to cover all the direct and indirect costs of a project. </rdfs:comment>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasApplicationDeadline">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:comment xml:lang="en">The date by which an application, for example a job application, a fellowship application or a grant application, has to be submitted if it is to be considered for acceptance or funding.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Application"/>
    <rdfs:label xml:lang="en">has application deadline</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <dc:description xml:lang="en">owl:equivalentProperty fabio:hasApplicationDeadline .</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasProjectIdentifier">
    <rdfs:label xml:lang="en">has project identifier</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property linking a project to an identifier for that project.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Project"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Scholarship">
    <rdfs:comment xml:lang="en">A financial grant to a scholar to enable him/her to undertake advanced academic study.</rdfs:comment>
    <rdfs:label xml:lang="en">scholarship</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Stipend"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/indirect-costs">
